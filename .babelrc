{
  "presets": ["next/babel"],
  "plugins": [
      ["module-resolver", {
        "root": ["./"],
        "extensions": [".js", ".jsx", ".ts", ".tsx", ".es", ".es6", ".mjs"],
      }],
      [
        "styled-components",
        {
          "ssr": true,
          "displayName": true,
          "preprocess": false
        }
      ]
    ],
  "env": {
    "development": {},
    "production": {},
    "test": {}
  }
}
